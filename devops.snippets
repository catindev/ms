

db.copyDatabase("MindSalesCRM", "MindSalesCRMtest")

mongodump --archive=mon01.archive --db MindSalesCRM
mongorestore --archive=mon16okt.archive --db MindSalesCRMtest

server {
    listen 80;
    server_name test.mindsales-crm.com;
    access_log /var/log/nginx/mindsales.log;
    location / {
        proxy_pass    http://127.0.0.1:6969/;
